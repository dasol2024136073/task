  printDan(6);
}

void printGuGuDan(int n, int m) {
   for (var x =n; x<=m; x++){
    for (var y=1; y<=9; y++){
    print("$x X $y = ${x * y}");
    }
    print('');
  }
}

void printDan(int n) {
  printGuGuDan(n,n);
