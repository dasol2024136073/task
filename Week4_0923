enum ColorType {
  RED(0),
  YELLOW(1),
  BLUE(2);
  
  final int code;
  const ColorType(this.code);
}

void main() {
  //var selectedColor = ColorType.YELLOW;
  
  var selectedColor = ColorType.RED;
  print(selectedColor.code);
  if (selectedColor == ColorType.RED) {
    print('빨강색!');
  }
}

