  var n = 20;
  var output = StringBuffer();
  
  for (int y = 0; y < n; y++){ //y는 세로길이
    for(var x = 0; x < n; x++){ //x는 가로길이
      var condition = true;
      if(condition) {
        output.write('#');
      } else{
        output.write(' ');
      }
    }
    output.writeln();
  }
  print(output.toString());

--------------------------------------------------------------------------------------------------------------------------

  var nums = [1, 2, 3, 4, 5];

  //인덱스가 감춰짐
  for (var num in nums) {
    print(num);
  }

  nums.forEach((num) => print(num));

  var condition = true;
  while (condition) {
    //while대신 do를 쓰면 false여도 무조건 한번은 실행함
    print('실행!');
    condition = false; //while을 쓰면 무한반복이기 때문에 무조건 빠져나오는 코드를 써야함
  }
  print('빠져나옴!');

------------------------------------------------------------------------------------------------------------------

  for(var i=0; i<5; i++){
    printHelloWorld('다솔');
    }
  
  for(var i=0; i<5; i++){
    printHelloWorld('솔다');
    }
 }

